# Enter your code here. Read input from STDIN. Print output to STDOUT
N=int(input())
for i in range(1,N+1):
    S=input()
    #N=len(S)
    even=''
    odd=''
    for i in range(len(S)):
        if i%2==0:
            even+=S[i]
        else:
            odd+=S[i]

    #even+=S[0]
    print(even+' '+odd)
    
        
